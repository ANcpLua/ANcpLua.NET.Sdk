<configuration>
  <packageSources>
    <clear />
    <add key="local" value="./artifacts" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
      <!-- ANcpLua.Roslyn.Utilities packages come from NuGet.org (upstream) -->
      <package pattern="ANcpLua.Roslyn.Utilities" />
      <package pattern="ANcpLua.Roslyn.Utilities.*" />
    </packageSource>
    <packageSource key="local">
      <!-- SDK packages come from local artifacts during build -->
      <package pattern="ANcpLua.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
