<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="local" value="artifacts/package/debug" />
  </packageSources>

  <!-- Package Source Mapping: ensures packages come from correct sources -->
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
      <package pattern="ANcpLua.*" />
    </packageSource>
    <packageSource key="local">
      <package pattern="ANcpLua.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
