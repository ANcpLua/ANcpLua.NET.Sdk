<?xml version="1.0"?>
<package>
  <metadata>
    <id>ANcpLua.NET.Sdk</id>
    <version>$version$</version>
    <authors>ANcpLua</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ANcpLua SDK for .NET projects</description>
    <readme>README.md</readme>
    <license type="expression">MIT</license>
    <repository type="git" url="$RepositoryUrl$" commit="$RepositoryCommit$" branch="$RepositoryBranch$"/>
  </metadata>
  <files>
    <file src="Sdk/ANcpLua.NET.Sdk/Sdk.props" target="Sdk/Sdk.props"/>
    <file src="Sdk/ANcpLua.NET.Sdk/Sdk.targets" target="Sdk/Sdk.targets"/>
    <file src="common/**/*" target="Sdk/common/"/>
    <file src="configuration/**/*" target="Sdk/configuration/"/>
    <file src="../eng/ANcpLua.Analyzers/bin/Release/netstandard2.0/ANcpLua.Analyzers.dll" target="Sdk/analyzers"/>
    <file src="../eng/ANcpLua.Analyzers/ANcpLua.Analyzers.editorconfig" target="Sdk/analyzers"/>

    <!-- Shared Sources -->
    <file src="../eng/Extensions/**/*" target="Sdk/shared/Extensions" exclude="**/README.md"/>
    <file src="../eng/LegacySupport/**/*" target="Sdk/shared/Polyfills" exclude="**/README.md"/>
    <file src="../eng/MSBuild/Polyfills/**/*" target="Sdk/shared/Polyfills" exclude="**/README.md"/>
    <file src="../eng/Shared/Throw/**/*" target="Sdk/shared/Throw" exclude="**/README.md"/>
    <file src="../eng/Shared/CodeTests/**/*" target="Sdk/shared/CodeTests" exclude="**/README.md"/>

    <file src="../LICENSE.txt" target=""/>
    <file src="../README.md" target=""/>
  </files>
</package>
