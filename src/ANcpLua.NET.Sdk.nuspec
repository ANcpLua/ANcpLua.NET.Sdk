<?xml version="1.0"?>
<package>
  <metadata>
    <id>ANcpLua.NET.Sdk</id>
    <version>$version$</version>
    <authors>ANcpLua</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ANcpLua SDK for .NET projects</description>
    <readme>README.md</readme>
    <license type="expression">MIT</license>
    <repository type="git" url="$RepositoryUrl$" commit="$RepositoryCommit$" branch="$RepositoryBranch$"/>
  </metadata>
  <files>
    <file src="Sdk/ANcpLua.NET.Sdk/Sdk.props" target="Sdk/Sdk.props"/>
    <file src="Sdk/ANcpLua.NET.Sdk/Sdk.targets" target="Sdk/Sdk.targets"/>
    <file src="common/**/*" target=""/>
    <file src="configuration/**/*" target=""/>
    <file src="../eng/ANcpLua.Analyzers/bin/Release/netstandard2.0/ANcpLua.Analyzers.dll" target="analyzers/"/>
    <file src="../eng/ANcpLua.Analyzers/ANcpLua.Analyzers.editorconfig" target="analyzers/"/>

    <!-- Shared Sources -->
    <file src="../eng/Extensions/**/*" target="shared/Extensions/" exclude="**/README.md"/>
    <file src="../eng/LegacySupport/**/*" target="shared/Polyfills/" exclude="**/README.md"/>
    <file src="../eng/MSBuild/Polyfills/**/*" target="shared/Polyfills/" exclude="**/README.md"/>
    <file src="../eng/Shared/Throw/**/*" target="shared/Throw/" exclude="**/README.md"/>
    <file src="../eng/Shared/CodeTests/**/*" target="shared/CodeTests/" exclude="**/README.md"/>

    <file src="../LICENSE.txt" target=""/>
    <file src="../README.md" target=""/>
  </files>
</package>
